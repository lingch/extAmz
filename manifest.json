{
  "name" : "Content settings",
  "version" : "0.2",
  "description" : "Shows the content settings for the current site.",
  "permissions": [ "contentSettings", "tabs" ],
  "browser_action": {
     "default_icon": "contentSettings.png",
     "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://www.mimigreat.com/forumdisplay.php*"],
      "css": ["mystyles.css"],
      "js": ["jquery-3.1.1.min.js", "myscript.js"]
    }
  ]
}
